<resources xmlns:tools="http://schemas.android.com/tools">
    <!-- Base application theme. -->
    <style name="Theme.FireAppYM" parent="Theme.AppCompat.Light.DarkActionBar">
        <!-- Primary brand color. -->
        <item name="colorPrimary">@color/SkiesBlue</item>
        <item name="colorPrimaryVariant">@color/SkiesBlue</item>
        <item name="colorOnPrimary">@color/white</item>
        <!-- Secondary brand color. -->
        <item name="colorSecondary">@color/teal_200</item>
        <item name="colorSecondaryVariant">@color/teal_700</item>
        <item name="colorOnSecondary">@color/black</item>
        <!-- Status bar color. -->
        <item name="android:statusBarColor" tools:targetApi="l">?attr/colorPrimaryVariant</item>
        <!-- Customize your theme here. -->
    </style>

    <style name="dateTimePicker" parent="android:Theme.Material.Light.Dialog">
        <item name="android:colorAccent">@color/SkiesBlue</item>
    </style>


    <style name="SplashTheme" parent="Theme.FireAppYM">
        <item name="android:windowBackground">@drawable/splashscreen</item>
    </style>
</resources>